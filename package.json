{
  "name": "mentorconnect",
  "version": "1.0.0",
  "private": true,
  "description": "A full-stack mentorship platform connecting mentors and mentees",
  "scripts": {
    "build:frontend": "cd frontend && npm run build",
    "dev:backend": "cd backend && python app.py",
